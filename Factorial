def factorial(num):
    # Your code goes here.
    result = 1;
    if(type(num) != int or num < 0):
        return None
    if(num == 1 or num == 0):
        return 1
    while num > 0:
        if(num == 1 or num == 0):
            result = result * 1
        else:
            result = result * num
        num = num - 1
    return result
    
